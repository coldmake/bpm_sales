<div class="row panel_item" data-no-turbolink>
  <div class="col-xs-12 col-md-offset-2 col-md-10">
    <p>Monthly quotation/invoice issued</p>
    <canvas id="canvas1" height="450" width="600"></canvas>


    <script>

        var barChartData = {
            labels: ["January", "February", "March", "April", "May", "June", "July"],
            datasets: [
                {
                    fillColor: "rgba(220,220,220,0.5)",
                    strokeColor: "rgba(220,220,220,1)",
                    data: [65, 59, 90, 81, 56, 55, 40]
                },
                {
                    fillColor: "rgba(151,187,205,0.5)",
                    strokeColor: "rgba(151,187,205,1)",
                    data: [28, 48, 40, 19, 96, 27, 100]
                }
            ]

        }

        var myLine = new Chart(document.getElementById("canvas1").getContext("2d")).Bar(barChartData);

    </script>
  </div>

  <div class="col-xs-12 col-md-offset-2 col-md-10">
    <p>Monthly customer growth</p>
    <canvas id="canvas2" height="450" width="600"></canvas>
    <script>

        var lineChartData = {
            labels: ["January", "February", "March", "April", "May", "June", "July"],
            datasets: [
                {
                    fillColor: "rgba(151,187,205,0.5)",
                    strokeColor: "rgba(151,187,205,1)",
                    pointColor: "rgba(151,187,205,1)",
                    pointStrokeColor: "#fff",
                    data: [28, 48, 50, 60, 80, 96, 100]
                }
            ]

        }

        var myLine = new Chart(document.getElementById("canvas2").getContext("2d")).Line(lineChartData);

    </script>

  </div>

  <div class="col-xs-12 col-md-offset-2 col-md-10">
    <p>Active customer ratio</p>
    <canvas id="canvas" height="450" width="450"></canvas>


  <script>

      var pieData = [
          {
              value: 30,
              color:"#F38630"
          },
          {
              value : 70,
              color : "#69D2E7"
          }

      ];

      var myPie = new Chart(document.getElementById("canvas").getContext("2d")).Pie(pieData);

  </script>
    </div>
</div>